def main():

    pickle_f = open( 'TFMatrix.dat', 'rb' )
    TFmat = pickle.load( pickle_f )
    pickle_f.close()


    f1 = TFmat['b'][0].Make_Formula(True, True)
    print f1.GetTitle()


    f2 = TFmat['b'][0].Make_Formula(False, True)
    print f2.GetTitle()


    



########################################
